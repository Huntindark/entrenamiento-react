{
  "name": "entrenamiento-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "-": "^0.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "styled-components": "5.2.3",
    "styled-icons": "^10.45.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.0-beta.7",
    "@storybook/addon-essentials": "^6.5.0-beta.7",
    "@storybook/addon-interactions": "^6.5.0-beta.7",
    "@storybook/addon-links": "^6.5.0-beta.7",
    "@storybook/builder-webpack5": "^6.5.0-beta.7",
    "@storybook/manager-webpack5": "^6.5.0-beta.7",
    "@storybook/node-logger": "^6.5.0-beta.7",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.5.0-beta.7",
    "@storybook/testing-library": "^0.0.11",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "storybook-react-router": "^1.0.8",
    "webpack": "^5.72.1"
  }
