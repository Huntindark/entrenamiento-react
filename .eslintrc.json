{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2, {"SwitchCase": 1}],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "double"]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  
